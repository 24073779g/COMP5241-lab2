{
  "version": 2,
  "builds": [
    {
      "src": "public/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/pages/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/index.html"
    }
  ]
}