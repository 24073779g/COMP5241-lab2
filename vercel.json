{
  "version": 2,
  "builds": [
    {
      "src": "public/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/public/main"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/pages/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/pages/index.html"
    }
  ]
}