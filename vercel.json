{
  "version": 2,
  "builds": [
    {
      "src": "public/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/pages/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/public/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/pages/index.html"
    }
  ],
  "functions": {
    "public/main.py": {
      "maxDuration": 30
    }
  }
}